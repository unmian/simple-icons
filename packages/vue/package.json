{
  "name": "@quarter/simple-icons",
  "version": "0.0.1",
  "author": {
    "name": "Quarter",
    "email": "master@shiruihua.cn"
  },
  "description": "Icons for Simple UI Kit",
  "keywords": [
    "vue2",
    "icons"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/unmian/simple-icons"
  },
  "files": [
    "lib",
    "types"
  ],
  "main": "lib/index.umd.js",
  "module": "lib/index.es.js",
  "exports": {
    ".": {
      "import": "./lib/index.es.js",
      "require": "./lib/index.umd.js"
    },
    "./lib/style.css": {
      "import": "./lib/style.css",
      "require": "./lib/style.css"
    }
  },
  "typings": "types/index.d.ts",
  "homepage": "https://github.com/unmian/simple-icons",
  "bugs": "https://github.com/unmian/simple-icons/issues",
  "scripts": {
    "dev": "vite --config ./build/base.config.ts",
    "lint": "f2elint scan",
    "test": "vue-tsc --skipLibCheck --noEmit",
    "build": "vue-tsc --skipLibCheck --noEmit && vite build",
    "preview": "vite preview",
    "update:deps": "node ./scripts/update.deps",
    "publish:private": "npm publish --registry https://npm.lescity.com.cn/",
    "unpublish:private": "npm unpublish --force --registry https://npm.lescity.com.cn/"
  },
  "peerDependencies": {},
  "devDependencies": {
    "@types/node": "^18.0.1",
    "@vue/compiler-sfc": "^3.2.37",
    "autoprefixer": "^10.4.7",
    "f2elint": "^2.2.1",
    "husky": "^8.0.1",
    "postcss": "^8.4.14",
    "sass": "^1.53.0",
    "typescript": "^4.7.4",
    "vite": "^2.9.13",
    "vite-plugin-dts": "^1.2.1",
    "vite-plugin-vue2": "^2.0.1",
    "vue": "^2.7.2",
    "vue-class-component": "^7.2.6",
    "vue-property-decorator": "^9.1.2",
    "vue-template-compiler": "^2.7.2",
    "vue-tsc": "^0.38.2"
  },
  "dependencies": {},
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "ie >= 10",
    "iOS >= 8",
    "Android >= 4"
  ]
}
